<openerp>
    <data>
        <record id="view_nh_etake_list_print_paper_take_list_wizard" model="ir.ui.view">
            <field name="name">Print Paper Take List Wizard</field>
            <field name="model">nh.etake_list.print_paper_take_list_wizard</field>
            <field name="arch" type="xml">
                <form string="Print Paper Take List Wizard" class= "oe_form_configuration" version="7.0">
                    <group>
                        <field name="sort_by" widget="selection"/>
                    </group>
                    <footer>
                        <button string="Print Report" type="object" name="print_report" class="oe_highlight"/>
                        <button string="Close" special="cancel" class="oe_link"/>
                    </footer>
                </form>
            </field>
        </record>

        <record id="action_nh_etake_list_print_paper_take_list_wizard" model="ir.actions.act_window">
            <field name="name">Print Paper Take List</field>
            <field name="res_model">nh.etake_list.print_paper_take_list_wizard</field>
            <field name="view_mode">form</field>
            <field name="view_type">form</field>
            <field name="target">new</field>
        </record>
    </data>
</openerp>