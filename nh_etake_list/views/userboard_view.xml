<openerp>
    <data>
        <!--Admin Userboard-->
        <record id="view_nhetakelist_admin_userboard_form" model="ir.ui.view">
            <field name="name">NH eObs Admin Userboard Form</field>
            <field name="model">nh.clinical.admin.userboard</field>
            <field name="inherit_id" ref="nh_clinical.view_nhc_admin_userboard_form"/>
            <field name="arch" type="xml">
                <xpath expr="//page[@string='Roles']" position="after">
                    <page string="eTake List Roles">
                        <group colspan="4" col="2">
                            <field name="receptionist"/>
                            <field name="referral_team"/>
                            <field name="junior_doctor"/>
                            <field name="registrar"/>
                            <field name="consultant"/>
                        </group>
                    </page>
                </xpath>
            </field>
        </record>
    </data>
</openerp>